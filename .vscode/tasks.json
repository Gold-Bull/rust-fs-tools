{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "check",
			"problemMatcher": [
				"$rustc"
			],
			"label": "Check"
		},
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "Debug Build"
		},
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"args": [
				"--release"
			],
			"label": "Release Build"
		},
		{
			"type": "cargo",
			"command": "clean",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "Clean"
		}
	]
}